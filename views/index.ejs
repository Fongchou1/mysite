  <!-- New Section 1 -->
  <section id="hero">
    <div class="hero-nameAndLogo">
      <img
        class="hero-nameAndLogo-logo"
        src="<%= images.logo.attachment[0].url %>"
      >
      <div class="hero-nameAndLogo-name ubuntu-9xl-144px-bold">
        <%= rootTexts['my-name'].en %>
      </div>
    </div>
    <div class="hero-infoAndDescription">
      <div class="hero-infoAndDescription-info">
        <div class="hero-infoAndDescription-info-email ubuntu-body1-18px-regular">
          <p>
            <%= rootTexts.email.en %>
          </p>
        </div>
        <div class="hero-infoAndDescription-info-title ubuntu-h5-24px-bold">
          <div class="hero-infoAndDescription-info-title-1">
            <%= rootTexts['roll-1'].en %>
          </div>
          <div class="hero-infoAndDescription-info-title-2">
            <%= rootTexts['roll-2'].en %>
          </div>
        </div>
      </div>
      <div class="hero-infoAndDescription-description">
        <div class="hero-infoAndDescription-description-aboutMe ubuntu-body1-18px-regular">
          <%= rootTexts['what-i-do'].en %>
        </div>
        <div class="hero-infoAndDescription-description-links ubuntu-body1-18px-regular">
          <div class="hero-infoAndDescription-description-links-link">
            <a class="btn btn-link" href=<%= links.instagram.url %> target='_blank' rel="noreferrer noopener" role="button" >
              <%= rootTexts['extra-link-1'].en %>
            </a>
          </div>
          <div class="hero-infoAndDescription-description-links-link">
            <a class="btn btn-link" href=<%= links.linkedin.url %> target='_blank' rel="noreferrer noopener" role="button" >
              <%= rootTexts['extra-link-2'].en %>
            </a>
          </div>
          <div class="hero-infoAndDescription-description-links-link">
            <a class="btn btn-link" href=<%= links.photography.url %> target='_blank' rel="noreferrer noopener" role="button" >
              <%= rootTexts['extra-link-3'].en %>
            </a>
          </div>
          <div class="hero-infoAndDescription-description-links-link">
            <a class="btn btn-link" href=<%= links.illustration.url %> target='_blank' rel="noreferrer noopener" role="button" >
              <%= rootTexts['extra-link-4'].en %>
            </a>
          </div>
        </div>
      </div>
    </div>
    <div class="hero-hr">
      <div class='hr-main'></div>
    </div>
  </section>

  <!-- New Section 2 -->
  <section id="projects">
    <% for( let i = 0; i < Object.values(projects).length; i++ ) { %>
    
      <div class="projects-project">
        <div class="projects-project-texts">
          <div class="projects-project-texts-title ubuntu-body1-18px-medium">
            <%= rootTexts['project-label'].en %>
          </div>
          <div class="projects-project-texts-content">
            <div class="projects-project-texts-content-title ubuntu-h3-48px-bold">
              <%= Object.values(projects)[i]['title-en'] %>
            </div>
            <div class="projects-project-texts-content-subtitle">
              <%= Object.values(projects)[i]['subtitle-en'] %>
            </div>
            <div class="projects-project-texts-content-description">
              <%= Object.values(projects)[i]['description-en'] %>
            </div>
            <div class="projects-project-texts-content-button">
              <a 
                class="btn btn-link" 
                href=<%= Object.values(projects)[i].url %>
                rel="noreferrer noopener" 
                role="button"
              >
                <%= rootTexts['project-button'].en %> &nbsp; <i class="fa-solid fa-arrow-right-long"></i>
              </a>
            </div>
          </div>
        </div>
        <div class="projects-project-device">
          <% if ( Object.values(projects)[i].device === 'laptop' ) { %>
            <div class="projects-project-device-macbook">
              <div class="projects-project-device-macbook-maskGroup-videoHolder">
                <video class="projects-project-device-macbook-maskGroup-video" autoplay muted loop>
                  <source 
                    src=<%= Object.values(projects)[i]['attachment'][0].url %>
                    type="video/mp4"
                  >
                  <source
                    src=<%= Object.values(projects)[i]['attachment'][0].url %>
                    type="video/ogg"
                  >
                Your browser does not support the video tag.
                </video>
              </div>
              <div 
                class="projects-project-device-macbook-maskGroup"
                style="
                  mask-image: url(<%= images['macbook-mask'].attachment[0].url %>);
                  -webkit-mask-image: url(<%= images['macbook-mask'].attachment[0].url %>);
                  -webkit-mask-repeat: no-repeat;
                  mask-repeat: no-repeat;
                  mask-size: 100%;
                  -webkit-mask-size: 100%;  
                "
              >
                <img
                  class="projects-project-device-macbook-macbookBody"
                  src=<%= images['macbook-body'].attachment[0].url %>
                  <!-- mask="url(#mask)" -->
                >
              </div>
            </div>
          <% } else if ( Object.values(projects)[i].device === 'mobile' ) { %>
            <div class="projects-project-device-mobile">
              <div class="projects-project-device-mobile-videoHolder">
                <video class="projects-project-device-mobile-videoHolder-video" autoplay muted loop>
                  <source 
                    src=<%= Object.values(projects)[i]['attachment'][0].url %>
                    type="video/mp4"
                  >
                  <source
                    src=<%= Object.values(projects)[i]['attachment'][0].url %>
                    type="video/ogg"
                  >
                Your browser does not support the video tag.
                </video>
              </div>
              <div 
                class="projects-project-device-mobile-maskGroup"
                style="
                  mask-image: url(<%= images['mobile-mask'].attachment[0].url %>);
                  -webkit-mask-image: url(<%= images['mobile-mask'].attachment[0].url %>);
                  -webkit-mask-repeat: no-repeat;
                  mask-repeat: no-repeat;
                  mask-size: 100%;
                  -webkit-mask-size: 100%;  
                "
              >
                <img
                  class="projects-project-device-mobile-mobileBody"
                  src=<%= images['mobile-body'].attachment[0].url %>
                >
              </div>
            </div>
          <% } %>
        </div>
      </div>
      <div class="projects-hr">
        <div class='hr-secondary'></div>
      </div>
    <% } %>
  </section>

  <!-- New Section 3 -->
  <section id="aboutMe">
    <div class="aboutMe-header ubuntu-h3-48px-bold">
      <%= rootTexts['about-title'].en %>
    </div>
    <div class="aboutMe-content">
      <div class="aboutMe-content-portraitHolder">
        <img 
          class="aboutMe-content-portraitHolder-portrait"
          src=<%= images['fong-photo'].attachment[0].url %>
          alt=<%= images['fong-photo'].alt %>
        >
      </div>
      <div class="aboutMe-content-texts">
        <div class="aboutMe-content-texts-title ubuntu-h5-24px-bold">
          <%= rootTexts['about-greeting'].en %>
        </div>
        <div class="aboutMe-content-texts-description ubuntu-body1-18px-light">
          <%= rootTexts['about-description'].en %>
        </div>
      </div>
    </div>
    <div class="aboutMe-hr">
      <div class='hr-tertiary'></div>
    </div>
  </section>

  <!-- New Section 4 -->
  <section id="services">
    <div class="services-header">
      <div class="services-header-title ubuntu-h3-48px-bold">
        <%= rootTexts['services-title'].en %>
      </div>
      <div class="services-header-description ubuntu-body1-18px-light">
        <%= rootTexts['services-content'].en %>
      </div>
    </div>
    <div class="services-content">
      <div class="services-content-group">
        <div class="services-content-group-item">
          <div class="services-content-group-item-title ubuntu-h5-24px-bold">
            <%= rootTexts['services-item-title-1'].en %>
          </div>
          <div class="services-content-group-item-description ubuntu-body1-18px-light">
            <%= rootTexts['services-item-description-1'].en %>
          </div>
        </div>
        <div class="services-content-group-item">
          <div class="services-content-group-item-title ubuntu-h5-24px-bold">
            <%= rootTexts['services-item-title-2'].en %>
          </div>
          <div class="services-content-group-item-description ubuntu-body1-18px-light">
            <%= rootTexts['services-item-description-2'].en %>
          </div>
        </div>
      </div>
      <div class="services-content-group">
        <div class="services-content-group-item">
          <div class="services-content-group-item-title ubuntu-h5-24px-bold">
            <%= rootTexts['services-item-title-3'].en %>
          </div>
          <div class="services-content-group-item-description ubuntu-body1-18px-light">
            <%= rootTexts['services-item-description-3'].en %>
          </div>
        </div>
        <div class="services-content-group-item">
          <div class="services-content-group-item-title ubuntu-h5-24px-bold">
            <%= rootTexts['services-item-title-4'].en %>
          </div>
          <div class="services-content-group-item-description ubuntu-body1-18px-light">
            <%= rootTexts['services-item-description-4'].en %>
          </div>
        </div>
      </div>
    </div>
    <div class="services-hr">
      <div class='hr-tertiary'></div>
    </div>
  </section>

