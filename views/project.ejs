            <div class="project-cta-hr">
                <div class='hr-main'></div>
            </div>
            <div class="project-cta-title ubuntu-h3-48px-bold">
                <%= rootTexts['cta-title'].en %>
            </div>
            <div class="project-cta-description">
    
            </div>
            <div class="project-cta-button">
                <a 
                    class="project-cta-button-text-primary" 
                    href=<%= links['cta-button'].url %> 
                    rel="noreferrer noopener" 
                    role="button"
                    target='_blank'
                >
                    <button type="button" class="btn btn-primary" >
                        <%= rootTexts['cta-button'].en %>
