                        <!-- device 1 -->
                        <div class="project-projects-mobile3Image-content-deviceHolder">
                            <div class="project-projects-mobile3Image-content-deviceHolder-imageHolder">
                              <img 
                                    class="project-projects-mobile3Image-content-deviceHolder-imageHolder-image"
                                    src=<%= Object.values(sections)[i]['mobileMedia-1'][0].url %>
                                    alt=<%= Object.values(sections)[i]['mobileMedia-1-alt'] %>
                                >
                            </div>
                            <div 
                              class="project-projects-mobile3Image-content-deviceHolder-maskGroup"
                              style="
                                mask-image: url(<%= images['mobile-mask-iphone12'].attachment[0].url %>);
                                -webkit-mask-image: url(<%= images['mobile-mask-iphone12'].attachment[0].url %>);
                                -webkit-mask-repeat: no-repeat;
                                mask-repeat: no-repeat;
                                mask-size: 100%;
                                -webkit-mask-size: 100%;  
                              "
                            >
                              <img
                                class="project-projects-mobile3Image-content-deviceHolder-maskGroup-mobileBody-iphone12"
                                src=<%= images['mobile-body-iphone12'].attachment[0].url %>
                              >
                            </div>
                        </div>
                        <!-- device 2 -->
                        <div class="project-projects-mobile3Image-content-deviceHolder">
                            <div class="project-projects-mobile3Image-content-deviceHolder-imageHolder">
                              <img 
                                    class="project-projects-mobile3Image-content-deviceHolder-imageHolder-image"
