                        </div>
                        <!-- device 2 -->
                        <div class="project-projects-mobile3Video-content-deviceHolder">
                            <div class="project-projects-mobile3Video-content-deviceHolder-videoHolder">
                                <video class="project-projects-mobile3Video-content-deviceHolder-videoHolder-video" defaultMuted oncontextmenu="return false;" preload="auto" playsinline autoplay muted loop>
                                    <source 
