<%- include("header") -%>
<body class="mine">

    <div id="globalLightTransitionCover">
        <div class="globalLightTransitionCover-animation-box">
            <div class="globalLightTransitionCover-animation-spinContainer">
            <div class="globalLightTransitionCover-animation-holder">
                <div class="globalLightTransitionCover-animation-holder-bd">
                <img 
                    class="globalLightTransitionCover-animation-img"
                    src='<%= images.loading.attachment[0].url %>'
                    alt='<%= images.loading.alt %>'
                >
                </div>
            </div>
            </div>
        </div>
    </div>

    <nav class="navbar navbar-expand-lg bg-dark navbar-dark">
        <div class="container-fluid">
            <a 
                class="btn btn-link navbar navbar-expand-lg bg-dark" 
                onclick='redirect(`<%= links.home.url %>`)'
                rel="noreferrer noopener" 
                role="button"
              >
                <i class="fa-solid fa-arrow-left-long"></i>  &nbsp;  <%= rootTexts.home.en %>
            </a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto ms-auto mb-2 mb-lg-0">
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle active" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                            <%= rootTexts['active-page'].en %>
                        </a>
                        <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <li>
                                <a 
                                    class="dropdown-item" 
                                    onclick='redirect(`<%= links['nav-page-1'].url %>`)'
                                >
                                    <%= rootTexts['nav-page-1'].en %>
                                </a>
                            </li>
                            <li>
                                <a 
                                    class="dropdown-item" 
                                    onclick='redirect(`<%= links['nav-page-2'].url %>`)'
                                >
                                    <%= rootTexts['nav-page-2'].en %>
                                </a>
                            </li>
                            <li>
                                <a 
                                    class="dropdown-item" 
                                    onclick='redirect(`<%= links['nav-page-3'].url %>`)'
                                >
                                    <%= rootTexts['nav-page-3'].en %>
                                </a>
                            </li>
                            <li>
                                <a 
                                    class="dropdown-item" 
                                    onclick='redirect(`<%= links['nav-page-4'].url %>`)'
                                >
                                    <%= rootTexts['nav-page-4'].en %>
                                </a>
                            </li>
                            <!-- <li>
                                <hr class="dropdown-divider">
                            </li> -->
                        </ul>
                    </li>
                    <!-- <li class="nav-item">
                        <a class="nav-link disabled">Disabled</a>
                    </li> -->
                </ul>
                <div>
                    <ul class="navbar-nav me-auto ms-auto mb-2 mb-lg-0">
                        <li class="nav-item px-1">
                            <a 
                                class="btn btn-link navbar navbar-expand-lg bg-dark" 
                                href=<%= links.instagram.url %>
                                rel="noreferrer noopener" 
                                role="button"
                                target='_blank'
                            >
                                <%= rootTexts['instagram'].en %>
                            </a>
                        </li>
                        <li class="nav-item px-1">
                            <a 
                                class="btn btn-link navbar navbar-expand-lg bg-dark" 
                                href=<%= links.linkedin.url %>
                                rel="noreferrer noopener" 
                                role="button"
                                target='_blank'
                            >
                                <%= rootTexts['linkedin'].en %>
                            </a>
                        </li>
                        <li class="nav-item px-1">
                            <a 
                                class="btn btn-link navbar navbar-expand-lg bg-dark" 
                                onclick='redirect(`<%= links.photography.url %>`)'
                                rel="noreferrer noopener" 
                                role="button"
                            >
                                <%= rootTexts['photography'].en %>
                            </a>
                        </li>
                        <li class="nav-item px-1">
                            <a 
                                class="btn btn-link navbar navbar-expand-lg bg-dark" 
                                onclick='redirect(`<%= links.graphics.url %>`)'
                                rel="noreferrer noopener" 
                                role="button"
                            >
                                <%= rootTexts['graphics'].en %>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </nav>

    <section id="project-hero">
        <div class="project-hero-content">
            <div class="project-hero-content-header">
                <div class="project-hero-content-header-title ubuntu-h3-48px-bold">
                    <%= rootTexts['project-title'].en %>
                </div>
                <div class="project-hero-content-header-subtitle">
                    <%= rootTexts['project-subtitle'].en %>
                </div>
                <div class="project-hero-content-header-description">
                    <%= rootTexts['project-description'].en %>
                </div>
            </div>
            <% if ( rootTexts['project-assignment'] && rootTexts['project-objective'] && rootTexts['project-scope'] && rootTexts['project-yearTitle'] ) { %>
                <div class="project-hero-content-features">
                    <div class="project-hero-content-features-item">
                        <div class="project-hero-content-features-item-title ubuntu-body1-18px-bold">
                            <%= rootTexts['project-assignment'].en %>
                        </div>
                        <div class="project-hero-content-features-item-content">
                            <%= rootTexts['project-assignment-content'].en %>
                        </div>
                    </div>
                    <div class="project-hero-content-features-item">
                        <div class="project-hero-content-features-item-title ubuntu-body1-18px-bold">
                            <%= rootTexts['project-objective'].en %>
                        </div>
                        <div class="project-hero-content-features-item-content">
                            <%= rootTexts['project-objective-content'].en %>
                        </div>
                    </div>
                    <div class="project-hero-content-features-item">
                        <div class="project-hero-content-features-item-title ubuntu-body1-18px-bold">
                            <%= rootTexts['project-scope'].en %>
                        </div>
                        <div class="project-hero-content-features-item-content">
                            <%= rootTexts['project-scope-content'].en %>
                        </div>
                    </div>
                    <div class="project-hero-content-features-item">
                        <div class="project-hero-content-features-item-title ubuntu-body1-18px-bold">
                            <%= rootTexts['project-yearTitle'].en %>
                        </div>
                        <div class="project-hero-content-features-item-content">
                            <%= rootTexts['project-year'].en %>
                        </div>
                    </div>
                </div>
            <% } %>
        </div>
        <div class="project-hero-hr">
            <div class='hr-main'></div>
        </div>
    </section>

    <section id="project-projects">
        <% for( let i = 0; i < Object.values(sections).length; i++ ) { %>
            <% if ( Object.values(sections)[i].type === 'simpleVideo' ) { %>
                <div class="project-projects-simpleVideo">
                    <div class="project-projects-simpleVideo-videoHolder">
                        <video class="project-projects-simpleVideo-video" defaultMuted oncontextmenu="return false;" preload="auto" autoplay muted loop playsinline>
                            <source 
                              src=<%= Object.values(sections)[i]['attachment'][0].url %>
                              type="video/mp4"
                            >
                            <source
                              src=<%= Object.values(sections)[i]['attachment'][0].url %>
                              type="video/ogg"
                            >
                          Your browser does not support the video tag.
                        </video>
                    </div>
                </div>
            <% } else if( Object.values(sections)[i].type === 'simpleImage' ) { %>
                <div class="project-projects-simpleImage">
                    <div class="project-projects-simpleImage-imageHolder">
                        <img 
                            class="project-projects-simpleImage-image"
                            src=<%= Object.values(sections)[i]['attachment'][0].url %>
                            alt=<%= Object.values(sections)[i].alt %>
                        >
                    </div>
                </div>
            <% } else if ( Object.values(sections)[i].type === 'text' ) { %>
                <div class="project-projects-text">
                    <div class="project-projects-text-hr">
                        <div class='hr-main'></div>
                    </div>
                    <div class="project-projects-text-title ubuntu-h3-48px-bold">
                        <%= Object.values(sections)[i]['title-en'] %>
                    </div>
                    <div class="project-projects-text-description">
                        <%= Object.values(sections)[i]['description-en'] %>
