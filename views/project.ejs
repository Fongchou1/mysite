                                        type="video/mp4"
                                    >
                                    <source
                                        src=<%= Object.values(sections)[i]['mobileMedia-1'][0].url %>
                                        type="video/ogg"
                                    >
                                    <%= Object.values(sections)[i]['mobileMedia-1-alt'] %>
