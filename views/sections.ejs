      <!-- section body -->
      <div class="largeText-body"> <!-- section body -->
        <!-- section container start -->
        <div class="largeText-container"> <!-- section container start -->
          <!-- icon start -->
          <div class="largeText-icon">
            <i class="fa-solid fa-arrow-right-long fa-3x"></i> &nbsp;
          </div><!-- icon start -->
          <!-- icon end -->
          <!-- texts start -->
          <div class="largeText-texts">
            <!-- title start -->
            <% if ( Object.values(sections)[i]['title-' + language] ) { %>
              <div class="ubuntu-h3-48px-bold">
                <% let words = Object.values(sections)[i]['title-' + language].split(' '); %>
                <% let wordHierarchy = 'normal' %>
                <% let currWord = '' %>
                <% for(let i = 0; i < words.length; i++ ) { %>
      
                  <% currWord = words[i]; %>
                  <% if ( words[i].includes(':normal:') ) { %>
                    <% wordHierarchy = 'normal' %>
                    <% currWord = words[i].replace(':normal:','') %>
                  <% } %>
                  <% if ( words[i].includes(':emphasize:') ) { %>
                    <% wordHierarchy = 'emphasize' %>
                    <% currWord = words[i].replace(':emphasize:','') %>
                  <% } %>
      
                  <% if ( wordHierarchy === 'normal' ) { %>
                    <span class="normal"><%= currWord %></span>
                  <% } %>
                  <% if ( wordHierarchy === 'emphasize' ) { %>
                    <span class="emphasize"><%= currWord %></span>
                  <% } %>
  
                <% } %>
                  </div>
            <% } %><!-- title start -->
            <!-- title end -->
            <!-- description start -->
            <% if ( Object.values(sections)[i]['subtitle-' + language] ) { %>
              <div class="ubuntu-body1-18px-regular">
                <% let words = Object.values(sections)[i]['subtitle-' + language].split(' '); %>
                <% let wordHierarchy = 'normal' %>
                <% let currWord = '' %>
                <% for(let i = 0; i < words.length; i++ ) { %>
      
                  <% currWord = words[i]; %>
                  <% if ( words[i].includes(':normal:') ) { %>
                    <% wordHierarchy = 'normal' %>
                    <% currWord = words[i].replace(':normal:','') %>
                  <% } %>
                  <% if ( words[i].includes(':emphasize:') ) { %>
                    <% wordHierarchy = 'emphasize' %>
                    <% currWord = words[i].replace(':emphasize:','') %>
                  <% } %>
      
                  <% if ( wordHierarchy === 'normal' ) { %>
                    <span class="normal"><%= currWord %></span>
                  <% } %>
                  <% if ( wordHierarchy === 'emphasize' ) { %>
                    <span class="emphasize"><%= currWord %></span>
                  <% } %>
  
                <% } %>
              </div>
            <% } %>
            <!-- description end -->
          </div><!-- texts start -->
          <!-- texts end -->
        </div> <!-- section container start -->
        <!-- section container end -->
      </div> <!-- section body -->
      <!-- section body -->
    </div> <!-- section start -->
    <!-- section end -->
  <% } %>
    
