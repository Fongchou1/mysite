                  <%= Object.values(sections)[i]['action-' + linksCount] %>
                </button>
            </a>
          </div>
          <!-- text-content end -->
          <!-- image start -->
          <div class="LargeTextAndImage-imageBox">
            <img
              class="LargeTextAndImage-img"
              src="<%= Object.values(sections)[i]['attachment'][0].url %>"
              alt="<%= Object.values(sections)[i]['alt'] %>"
            >
          </div>
          <!-- image end -->
        </div>
        <!-- content body end -->
        <!-- divider start -->
        <div class='hr'></div>
        <!-- divider end -->
      </div>
      <!-- section container end -->
    </div>
    <!-- section end -->
  <% } %>
    
  <% if ( Object.values(sections)[i].type === 'largeText' ) { %>
