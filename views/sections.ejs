          <% for( let j = 0; j < Object.values(sections)[i]['card-links'].length; j++ ) { %>
            <% let layoutString = '' %>
            <% if( currNumber % 3 == 1 ) { %>
              <% layoutString = 'first' %>
            <% } else if ( currNumber % 3 == 2 ) { %>
              <% layoutString = 'second' %>
            <% } else if ( currNumber % 3 == 0 ) { %>
