      <!-- section container start -->
      <div class="section-LargeTextAndImage-content">
        <!-- nav start -->
        <nav class="section-LargeTextAndImage-nav navbar navbar-expand-lg navbar-light">
          <div class="container-fluid">
            <% if(Object.values(sections)[i]['action-1-link'] && images.logo.attachment[0]){ %>
              <a class="navbar-brand" href=<%= Object.values(sections)[i]['action-1-link'] %> >
                <img
                  class="section-LargeTextAndImage-content-logo"
                  src="<%= images.logo.attachment[0].url %>"
                >
              </a>
            <% } %>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
              <%= rootTexts['menu'][language] %>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
              <ul class="navbar-nav">
                <% let linksCount = 1 %>
                <%while ( linksCount <= 4 && Object.values(sections)[i]['action-' + linksCount + '-link'] ) { %>
                  <% let stateClass = '' %>
                  <% let label = '' %>
                  <% if( Object.values(sections)[i]['action-' + linksCount].includes(':active') ) { %>
                    <% stateClass = 'nav-link active' %>
                    <% Object.values(sections)[i]['action-' + linksCount].indexOf(":active") %>
                    <% label = Object.values(sections)[i]['action-' + linksCount].substring(0, Object.values(sections)[i]['action-' + linksCount].indexOf(":active") ) %>
                  <% } else { %>
                    <% stateClass = 'nav-link' %>
                    <% label = Object.values(sections)[i]['action-' + linksCount] %>
                  <% } %>
                  <li class="nav-item">
                    <a 
                      class='<%= stateClass %>' 
                      aria-current="page" 
                      href=<%= Object.values(sections)[i]['action-' + linksCount + '-link'] %> ><%= label %>
                    </a>
                  </li>
                  <% linksCount++ %>
                <% } %>
              </ul>
            </div>
          </div>
        </nav>
        <!-- nav end -->
        <!-- content body start -->
        <div class="body">
          <!-- text-content start -->
          <div class="text-content">
            <div class="ubuntu-h3-48px-bold">
              <%= Object.values(sections)[i]['title-' + language] %>
            </div>
            <div class="ubuntu-h5-24px-regular">
              <%= Object.values(sections)[i]['subtitle-' + language] %>
            </div>
            <a 
                class="project-cta-button-text-primary" 
                href=<%= Object.values(sections)[i]['action-' + linksCount + '-link'] %> 
                rel="noreferrer noopener" 
                role="button"
                target='_blank'
            >
