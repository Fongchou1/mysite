<%- include("header") -%>
<body class="mine">
  <div>

  <%- include("loading") -%>
  <nav class="navbar navbar-expand-lg bg-dark navbar-dark">
      <div class="container-fluid">
          <a 
              class="btn btn-link navbar navbar-expand-lg bg-dark" 
              onclick='redirect(`<%= links.home.url %>`)' 
              rel="noreferrer noopener" 
              role="button"
            >
              <i class="fa-solid fa-arrow-left-long"></i>  &nbsp;  <%= rootTexts.home.en %>
          </a>
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
              <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarSupportedContent">
              <div class="navbar-nav me-auto ms-auto mb-2 mb-lg-0 link-400" >
                  <%= rootTexts['active-page'].en %>
              </div>
              <div>
                  <ul class="navbar-nav me-auto ms-auto mb-2 mb-lg-0">
                      <li class="nav-item px-1">
                          <a 
                              class="btn btn-link navbar navbar-expand-lg bg-dark" 
                              href=<%= links.instagram.url %>
                              rel="noreferrer noopener" 
                              role="button"
                              target='_blank'
                          >
                              <%= rootTexts['instagram'].en %>
                          </a>
                      </li>
                      <li class="nav-item px-1">
                          <a 
                              class="btn btn-link navbar navbar-expand-lg bg-dark" 
                              href=<%= links.linkedin.url %>
                              rel="noreferrer noopener" 
                              role="button"
                              target='_blank'
                          >
                              <%= rootTexts['linkedin'].en %>
                          </a>
                      </li>
                      <li class="nav-item px-1">
                          <a 
                              class="btn btn-link navbar navbar-expand-lg bg-dark" 
                              onclick='redirect(`<%= links.photography.url %>`)' 
                              rel="noreferrer noopener" 
                              role="button"
                          >
                              <%= rootTexts['photography'].en %>
                          </a>
                      </li>
                      <li class="nav-item px-1">
                          <a 
                              class="btn btn-link navbar navbar-expand-lg bg-dark" 
                              onclick='redirect(`<%= links.design.url %>`)' 
                              rel="noreferrer noopener" 
                              role="button"
                          >
                              <%= rootTexts['design'].en %>
                          </a>
                      </li>
                  </ul>
              </div>
          </div>
      </div>
  </nav>

    <section id="project-hero">
        <div class="project-hero-content">
            <div class="project-hero-content-header">
                <div class="project-hero-content-header-title ubuntu-h3-48px-bold">
                    <%= rootTexts['project-title'].en %>
                </div>
                <div class="project-hero-content-header-subtitle">
                    <%= rootTexts['project-subtitle'].en %>
                </div>
                <div class="project-hero-content-header-description">
                    <%= rootTexts['project-description'].en %>
                </div>
            </div>
        </div>
        <div class="project-hero-hr">
            <div class='hr-main'></div>
        </div>
    </section>

    <section id="project-projects" >
        <% for( let i = 0; i < Object.values(sections).length; i++ ) { %>
            <% if ( Object.values(sections)[i].type === 'simpleVideo' ) { %>
                <div class="project-projects-simpleVideo">
                    <div class="project-projects-simpleVideo-videoHolder">
                        <video class="project-projects-simpleVideo-video" defaultMuted oncontextmenu="return false;" preload="auto" playsinline autoplay muted loop>
                            <source 
                              src=<%= Object.values(sections)[i]['attachment'][0].url %>
                              type="video/mp4"
                            >
                            <source
                              src=<%= Object.values(sections)[i]['attachment'][0].url %>
                              type="video/ogg"
                            >
                          Your browser does not support the video tag.
                        </video>
                    </div>
                </div>
            <% } else if( Object.values(sections)[i].type === 'simpleImage' ) { %>
                <div class="project-projects-simpleImage">
                    <div class="project-projects-simpleImage-imageHolder">
                        <img 
                            class="project-projects-simpleImage-image"
                            src=<%= Object.values(sections)[i]['attachment'][0].url %>
                            alt=<%= Object.values(sections)[i].alt %>
                        >
                    </div>
                </div>
            <% } else if ( Object.values(sections)[i].type === 'text' ) { %>
                <div class="project-projects-text">
                    <div class="project-projects-text-hr">
                        <div class='hr-main'></div>
                    </div>
                    <div class="project-projects-text-title ubuntu-h3-48px-bold">
                        <%= Object.values(sections)[i]['title-en'] %>
                    </div>
                    <div class="project-projects-text-description">
                        <%= Object.values(sections)[i]['description-en'] %>
                    </div>
                </div>
            <% } else if ( Object.values(sections)[i].type === 'mobile3Image' ) { %>
                <div class="project-projects-mobile3Image">
                    <div class="project-projects-mobile3Image-content">
                        <!-- device 1 -->
                        <div class="project-projects-mobile3Image-content-deviceHolder">
                            <div class="project-projects-mobile3Image-content-deviceHolder-imageHolder">
                              <img 
                                    class="project-projects-mobile3Image-content-deviceHolder-imageHolder-image"
                                    src=<%= Object.values(sections)[i]['mobileMedia-1'][0].url %>
                                    alt=<%= Object.values(sections)[i]['mobileMedia-1-alt'] %>
                                >
                            </div>
                            <div 
                              class="project-projects-mobile3Image-content-deviceHolder-maskGroup"
                              style="
                                mask-image: url(<%= images['mobile-mask-iphone12'].attachment[0].url %>);
                                -webkit-mask-image: url(<%= images['mobile-mask-iphone12'].attachment[0].url %>);
                                -webkit-mask-repeat: no-repeat;
                                mask-repeat: no-repeat;
                                mask-size: 100%;
                                -webkit-mask-size: 100%;  
                              "
                            >
                              <img
                                class="project-projects-mobile3Image-content-deviceHolder-maskGroup-mobileBody-iphone12"
                                src=<%= images['mobile-body-iphone12'].attachment[0].url %>
                              >
                            </div>
                        </div>
                        <!-- device 2 -->
                        <div class="project-projects-mobile3Image-content-deviceHolder">
                            <div class="project-projects-mobile3Image-content-deviceHolder-imageHolder">
                              <img 
                                    class="project-projects-mobile3Image-content-deviceHolder-imageHolder-image"
                                    src=<%= Object.values(sections)[i]['mobileMedia-2'][0].url %>
                                    alt=<%= Object.values(sections)[i]['mobileMedia-2-alt'] %>
                                >
                            </div>
                            <div 
                              class="project-projects-mobile3Image-content-deviceHolder-maskGroup"
                              style="
                                mask-image: url(<%= images['mobile-mask-iphone12'].attachment[0].url %>);
                                -webkit-mask-image: url(<%= images['mobile-mask-iphone12'].attachment[0].url %>);
                                -webkit-mask-repeat: no-repeat;
                                mask-repeat: no-repeat;
                                mask-size: 100%;
                                -webkit-mask-size: 100%;  
                              "
                            >
                              <img
                                class="project-projects-mobile3Image-content-deviceHolder-maskGroup-mobileBody-iphone12"
                                src=<%= images['mobile-body-iphone12'].attachment[0].url %>
                              >
                            </div>
                        </div>
                        <!-- device 3 -->
