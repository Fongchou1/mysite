                        <div class="graphics-setOf3Images-content-imageHolder">
                            <img 
                                class="graphics-setOf3Images-content-imageHolder-image"
                                src=<%= Object.values(sections)[i]['setOf3Images-1'][0].url %>
                                alt=<%= Object.values(sections)[i]['setOf3Images-1-alt'] %>
                            >
                        </div>
                        <!-- device 2 -->
                        <div class="graphics-setOf3Images-content-imageHolder">
